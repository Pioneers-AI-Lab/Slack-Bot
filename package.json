{
    "name": "slack-calendar-autobot",
    "version": "0.9.0",
    "description": "Automated Slack digests from Google Calendar events",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "local": "npx vercel dev",
        "deploy": "npx vercel deploy",
        "test": "node --test",
        "call:daily": "node scripts/call-endpoint.js daily",
        "call:weekly": "node scripts/call-endpoint.js weekly",
        "call:daily:prod": "VERCEL_URL=${VERCEL_URL:-https://your-project.vercel.app} node scripts/call-endpoint.js daily",
        "call:weekly:prod": "VERCEL_URL=${VERCEL_URL:-https://your-project.vercel.app} node scripts/call-endpoint.js weekly"
    },
    "keywords": [
        "slack",
        "calendar",
        "automation",
        "serverless"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@slack/web-api": "^7.0.0",
        "dotenv": "^16.4.0",
        "googleapis": "^128.0.0",
        "zod": "^4.2.0"
    },
    "devDependencies": {
        "vercel": "^50.0.1"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
